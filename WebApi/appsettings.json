{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=questionable.db,shared=true"
  },
  "AllowedHosts": "*",
  "S3Settings": {
    "Logging": {
      "HttpRequests": false,
      "S3Requests": true,
      "SignatureV4Validation": true
    },
    "OperationLimits": {
      "MaxPutObjectSize": 134217728
    },
    "Webserver" : {
      "Hostname" : "localhost",
      "Port" : 8080,
      "DebugSettings" : {
        "AccessControl" :false,
        "Routing" : false,
        "Requests" : false,
        "Responses" : false
      }
    },
    "EnableSignatures" : true
  }
}